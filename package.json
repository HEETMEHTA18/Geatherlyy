{
  "name": "gatherly",
  "version": "1.0.0",
  "description": "Centralized Club Management System",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup:backend": "cd backend && npm install && npm run prisma:generate",
    "setup:frontend": "cd frontend && npm install",
    "setup:all": "npm run setup:backend && npm run setup:frontend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "cd backend && npm run prisma:migrate",
    "db:seed": "cd backend && npm run prisma:seed",
    "db:studio": "cd backend && npm run prisma:studio"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.0",
    "next": "^14.0.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
